# Meta-analysis (Primary analysis)
aki_incidence_meta <- metaprop(event = case_aki,
n = sample_size,
studlab = paste0(authors,", ", location),
data = aki,
sm = "PLO",
predict = TRUE,
hakn = TRUE,
comb.fixed = TRUE,
comb.random = TRUE,
level.comb = 0.95,
method.tau = "ML",
method.bias = "linreg",
warn = TRUE
)
